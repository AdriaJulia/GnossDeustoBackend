<body class="@ViewData["BodyClass"] comunidad usuarioConectado" cz-shortcut-listen="true">
    <header id="header">
        <div class="container-fluid">
            <div class="row">
                <div class="col col01">
                    
                    <div class="logo">
                        <a href="@Url.Action("Index", "RepositoryConfig")">Hércules</a>
                    </div>
                </div>
                <div class="col col02">
                    <ul>
                        <li>
                            <a asp-area="" asp-controller="RepositoryConfig" asp-action="Index">Repositorios</a>
                        </li>
                        <li>
                            <a asp-area="" asp-controller="ShapeConfig" asp-action="Index">Shapes</a>
                        </li>
                        <li>
