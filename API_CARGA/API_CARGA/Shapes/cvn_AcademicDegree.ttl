@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix bibo: <http://purl.org/roh/mirror/bibo/> .
@prefix foaf: <http://purl.org/roh/mirror/foaf#> .
@prefix obobfo: <http://purl.org/roh/mirror/obo/bfo#> .
@prefix oboro: <http://purl.org/roh/mirror/obo/ro#> .
@prefix roh: <http://purl.org/roh#> .
@prefix vivo: <http://purl.org/roh/mirror/vivo#> .
@prefix vcard: <http://purl.org/roh/mirror/vcard#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix geonames: <http://purl.org/roh/mirror/geonames#> .

	
roh:AcademicDegreeShape
	a sh:NodeShape ;
	sh:targetClass vivo:AcademicDegree;	
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:title;
		sh:qualifiedMinCount  1;
		sh:qualifiedValueShape [
			sh:datatype xsd:string ;
		] ;
    ] ;	
	sh:property [ 
		sh:severity sh:Violation;
		sh:path roh:title;
		sh:datatype xsd:string;
	] ;	
	sh:property [  
		sh:severity sh:Violation;
		sh:path roh:abbreviation;
		sh:class rdfs:Literal;
    ] ;
	sh:property [  
		sh:severity sh:Warning;
		sh:path roh:abbreviation;
		sh:qualifiedMinCount  1;
		sh:qualifiedValueShape [
			sh:datatype xsd:nonNegativeInteger ;
		] ;
    ] .
