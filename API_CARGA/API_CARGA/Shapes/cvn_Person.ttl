@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix bibo: <http://purl.org/roh/mirror/bibo/> .
@prefix foaf: <http://purl.org/roh/mirror/foaf#> .
@prefix obobfo: <http://purl.org/roh/mirror/obo/bfo#> .
@prefix oboero: <http://purl.org/roh/mirror/obo/ero#> .
@prefix oboro: <http://purl.org/roh/mirror/obo/ro/> .
@prefix roh: <http://purl.org/roh#> .
@prefix vivo: <http://purl.org/roh/mirror/vivo#> .
@prefix bibo: <http://purl.org/roh/mirror/bibo#> .
@prefix vcard: <http://purl.org/roh/mirror/vcard#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .




	
roh:PersonShape
	a sh:NodeShape ;
	sh:targetClass foaf:Person;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasCV;
		sh:class roh:CurriculumVitae;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasContactInfo;
		sh:class vcard:Individual;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasContract;
		sh:class roh:PersonContract;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasKnowledgeArea;
		sh:class skos:Concept;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasPosition;
		sh:class vivo:Position;
		
		sh:or ( 
			[ sh:class vivo:Position ] 
			[ sh:class vivo:FacultyAdministrativePosition ] 
			[ sh:class vivo:FacultyPosition ] 
			[ sh:class vivo:FacultyPositionEmeritus ] 
			[ sh:class vivo:LibrarianPosition ] 
			[ sh:class vivo:LibrarianPositionEmeritus ] 
			[ sh:class vivo:NonAcademicPosition ] 
			[ sh:class vivo:NonFacultyAcademicPosition ] 
			[ sh:class vivo:PostdocPosition ] 
			[ sh:class vivo:PrimaryPosition ] 
			[ sh:class vivo:ResearcherPosition ] 
			)
		
		
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:hasRole;		
		sh:or ( 
			[ sh:class obobfo:BFO_0000023 ] 
			[ sh:class vivo:AdministratorRole ] 
			[ sh:class vivo:AdviseeRole ] 
			[ sh:class vivo:AdvisorRole ] 
			[ sh:class vivo:AtendeeRole ] 
			[ sh:class roh:AuditeeRole ] 
			[ sh:class roh:AuditorRole ] 
			[ sh:class vivo:EditorRole ] 
			[ sh:class oboero:ERO_0000225 ] 
			[ sh:class oboero:ERO_0000787 ] 
			[ sh:class oboero:ERO_0000788 ] 
			[ sh:class oboero:ERO_0000789 ] 
			[ sh:class oboero:ERO_0000224 ] 
			[ sh:class vivo:LeaderRole ] 
			[ sh:class vivo:MemberRole ] 
			[ sh:class vivo:OrganizerRole ] 
			[ sh:class vivo:PresenterRole ] 
			[ sh:class vivo:ResearcherRole ] 
			[ sh:class vivo:InvestigatorRole ] 			
			[ sh:class vivo:CoPrincipalInvestigatorRole ] 
			[ sh:class vivo:PrincipalInvestigatorRole ] 
			[ sh:class vivo:ReviewerRole ] 
			[ sh:class oboero:ERO_0000783 ] 
			[ sh:class oboero:ERO_0000785 ] 
			[ sh:class oboero:ERO_0000786 ] 
			[ sh:class oboero:ERO_0000790 ] 
			[ sh:class oboero:ERO_0000914 ] 
			[ sh:class oboero:ERO_0000784 ] 
			[ sh:class roh:SuperviseeRole ] 
			[ sh:class roh:SupervisorRole ] 
			[ sh:class vivo:TeacherRole ] 
			[ sh:class vivo:ThirdPartyContractorRole ] 
			)
	] ;
	sh:property [	
		sh:severity sh:Violation;
		sh:path roh:participates;
		sh:or ( 
			[ sh:class roh:Activity ] 
			[ sh:class vivo:Competition ] 
			[ sh:class bibo:Conference ] 
			[ sh:class vivo:Course ] 
			[ sh:class vivo:Exhibit ] 
			[ sh:class bibo:Hearing ] 
			[ sh:class vivo:Internship ] 
			[ sh:class bibo:Interview ] 
			[ sh:class vivo:Meeting ] 
			[ sh:class bibo:Performance ] 
			[ sh:class vivo:Presentation ] 
			[ sh:class vivo:InvitedTalk ] 
			[ sh:class roh:PanelTalk ] 
			[ sh:class roh:ThesisViva ] 
			[ sh:class bibo:Workshop ] 
		)
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:spends;
		sh:class roh:PersonExpense;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path vivo:eligibleFor;
		sh:or ( 
			[ sh:class roh:Accreditation ] 
			[ sh:class roh:AcademicAccreditation ] 
			[ sh:class vivo:Certificate ] 
			[ sh:class roh:LanguageCertificate ] 
			[ sh:class roh:DegreeHomologation ] 
			[ sh:class vivo:License ] 
			[ sh:class roh:ResearchAccreditation ] 
		)
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:birthdate;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:researchLine;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path roh:taxID;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:firstName;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:gender;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:name;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:nick;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:surname;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path foaf:title;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path vivo:description;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path vivo:freetextKeyword;
		sh:datatype xsd:string;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path vivo:identifier;
		sh:datatype xsd:string;
	] ;
	sh:property [	
		sh:severity sh:Warning ;
		sh:path foaf:firstName;
		sh:qualifiedMinCount  1; 
		sh:qualifiedValueShape [
			sh:datatype xsd:string ;
		] ;
	] ;
	sh:property [
		sh:severity sh:Warning;
		sh:path foaf:surname;
		sh:qualifiedMinCount  1;
		sh:qualifiedValueShape [
			sh:datatype xsd:string ;
		] ;
	] ;
	sh:property [
		sh:severity sh:Warning;
		sh:path vivo:identifier;
		sh:qualifiedMinCount  1;
		sh:qualifiedValueShape [
			sh:datatype xsd:string ;
		] ;
	] ;
	sh:property [
		sh:severity sh:Violation;
		sh:path vivo:relatedBy;
		sh:or ( 
			[ sh:class vivo:Relationship ] 
			[ sh:class vivo:AdvisingRelationship ] 
			[ sh:class vivo:FacultyMentoringRelationship ] 
		)
	] .